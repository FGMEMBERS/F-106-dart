<?xml version="1.0" encoding="UTF-8"?>

<!-- Engine Pressure Ratio Gage -->

<PropertyList>

  <path>eprg.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>visse1</object-name>
    <object-name>visse2</object-name>
    <object-name>visse3</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine[0]/epr</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>   0 </dep></entry>
      <entry><ind> 1.2 </ind><dep>  17 </dep></entry>
      <entry><ind> 1.5 </ind><dep>  61 </dep></entry>
      <entry><ind> 2.0 </ind><dep> 134 </dep></entry>
      <entry><ind> 2.5 </ind><dep> 206 </dep></entry>
      <entry><ind> 3.0 </ind><dep> 278 </dep></entry>
      <entry><ind> 3.4 </ind><dep> 335 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

